{  
  "$id": "emergencyContact",
  "type": "object",
  "required": ["name", "dayPhone", "email", "relationship"],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 3,
      "isNotEmpty": true
    },
    "dayPhone": {
      "type": "string",
      "minLength": 1,
      "isNotEmpty": true
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "relationship": { 
      "type": "string",
      "enum": [
        "",
        "Dependent", 
        "Sibling", 
        "Parent", 
        "Friend", 
        "Guardian", 
        "Child",
        "Relative",
        "Spouse_Significant_Other"
      ]
    }
  },
  "additionalProperties": false
}
